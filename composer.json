{
    "name": "smallkingdev/worldtimeblocker",
    "description": "a simple plugin that blocks world time.",
    "type": "project",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "pocketmine/pocketmine-mp": "^4.0.0",
        "sylvrs/libmarshal": "^1.4.3",
        "muqsit/simplepackethandler": "master"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "muqsit/simplepackethandler",
                "version": "master",
                "autoload": {
                    "classmap": [
                        "src/"
                    ]
                },
                "source": {
                    "url": "https://github.com/muqsit/simplepackethandler",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "SmallkingDev\\WorldTimeBlocker\\": "src"
        }
    }
}